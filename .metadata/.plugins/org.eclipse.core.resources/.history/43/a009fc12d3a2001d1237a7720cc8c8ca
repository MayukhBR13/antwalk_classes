package day9;

@FunctionalInterface
interface MyInterface{
	Person getStudent(String s1,Integer age);
}

public class MethodRefCons {
	public static void main(String args[]) {
		//invoke default student getStudent()->default constructor
		MyInterface obj=Person::new;
		System.out.println(obj.getStudent("Mayukh",22));//obj.getStudent() is same as new Student()
	}
}
